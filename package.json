{
  "name": "speculaas",
  "version": "0.8.0",
  "description": "NodeJS implementation of clojure.spec",
  "keywords": [
    "specification",
    "testing"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive -R spec",
    "cover": "istanbul cover _mocha -- -R spec",
    "lint": "eslint lib examples",
    "stryker": "stryker run stryker.conf.js",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "npm run docs:prepare && rm -rf _book && gitbook build",
    "docs:publish": "npm run docs:build && gh-pages -d _book"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrijk/speculaas.git"
  },
  "pre-commit": [
    "test"
  ],
  "author": {
    "name": "Maurits Rijk",
    "email": "maurits.rijk@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/mrijk/speculaas/issues"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "testcheck": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.3.0",
    "@stryker-mutator/html-reporter": "^2.3.0",
    "@stryker-mutator/javascript-mutator": "^2.3.0",
    "@stryker-mutator/mocha-framework": "^2.3.0",
    "@stryker-mutator/mocha-runner": "^2.3.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-node": "^10.0.0",
    "genfun": "^5.0.0",
    "gh-pages": "^2.1.1",
    "gitbook-cli": "^2.3.2",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.2",
    "pre-commit": "^1.2.2",
    "sinon": "^7.5.0"
  }
}
